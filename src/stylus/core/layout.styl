@media all and (max-width window-lg)
	.hidden-lg
		display none !important
		visibility hidden
	
	.visible-lg
		display inherit !important
		visibility visible
	for i in (1..12)
		.lg_{i}
			width 100%

	for i in (1..12)
		.md_{i}
			col-width(i)
			float left
			&.inline-block
				float none
				display inline-block
				vertical-align top
				margin-left -4px
			
@media all and (max-width window-md)
	
	.hidden-md
		display none !important
		visibility hidden
	
	.visible-md
		display inherit !important
		visibility visible
		&.inline
			display inline-block !important
	for i in (1..12)
		.md_{i}
			width 100%
	for i in (1..12)
		.sm_{i}
			col-width(i)
			float left
			&.inline-block
				float none
				display inline-block
				vertical-align top
				margin-left -4px

@media all and (max-width window-sm)

	.hidden-sm
		display none !important
		visibility hidden

	.visible-sm
		display inherit !important
		visibility visible
	for i in (1..12)
		.sm_{i}
			width 100%

	for i in (1..12)
		.xs_{i}
			col-width(i)
			float left
			&.inline-block
				float none
				display inline-block
				vertical-align top
				margin-left -4px

@media all and (max-width window-xs)
	for i in (1..12)
		.xs_{i}
			width 100%
	.hidden-xs
		display none !important
		visibility hidden

	.visible-xs
		display inherit !important
		visibility visible

